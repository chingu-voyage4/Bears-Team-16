{
  "extends": "airbnb",
  "plugins": ["react"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "impliedStrict": true,
      "classes": true,
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "jquery": true
  },
  "rules": {
    "react/jsx-filename-extension": 0,
    "consistent-return": 1,
    "no-tabs": 0,
    "import/prefer-default-export": 0,
    "no-console": 0,
    "no-unused-vars": [1, { "vars": "local", "args": "none" }],
    "max-len": [2, { "code": 100, "tabWidth": 4 }],
    "arrow-parens": 0,
    "array-bracket-spacing": [2, "always"],
    "array-element-newline": [2, { "minItems": 3 }],
    "array-bracket-newline": [2, { "multiline": true }],
    "object-curly-spacing": [2, "always"],
    "quotes": [
      2,
      "backtick",
      {
        "avoidEscape": true,
        "allowTemplateLiterals": true
      }
    ]
  },
  "globals": {
    "document": true
  }
}
